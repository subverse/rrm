<%= render :partial => "shared/titles", :locals => {:tab => "rrm::Quellen (new/edit)", :page => "" }%>

<div id="form">
  <h2>Quelle</h2>
  <% form_for(@source) do |f| %>
    <%= f.error_messages %>
    <table>
      <tr>
        <td>Name</td>
        <td><%= f.text_field :name %></td>
      </tr>
    </table>
    <%= f.submit 'Bestätigen' %>
  <% end %>
<br/>
<hr/>
  <%= link_to 'Zurück', sources_path, :class => "link_ext" %>
</div>
